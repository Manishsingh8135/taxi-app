{
  "expo": {
    "name": "TaxiGo",
    "slug": "taxigo-rider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "taxigo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taxigo.rider",
      "config": {
        "googleMapsApiKey": "AIzaSyBovKxjpjTLfZRhTjX78bYViXuh4II6C7o"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "TaxiGo needs your location to show nearby drivers and navigate.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "TaxiGo needs your location to track your ride."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.taxigo.rider",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBovKxjpjTLfZRhTjX78bYViXuh4II6C7o"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-location",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": false
    }
  }
}
